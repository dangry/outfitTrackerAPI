    "version": "0.2.0",
    "configurations": [
        {
            "name": "Deno",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "deno",
            "runtimeArgs": ["run", "--inspect", "-A", "--allow-net", "--allow-write", "--allow-read", "server.ts" ],
            "port": 9229,
            "outputCapture": "std"
        }
    ]
}